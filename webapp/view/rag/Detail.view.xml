<mvc:View
    controllerName="gestionesop.controller.rag.Detail"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f"
    xmlns:c="sap.ui.core"
>
    <f:DynamicPage
        showFooter="true"
        class="sapUiNoContentPadding"
        stickySubheaderProvider="wizDetail"
    >
        <f:header>
            <f:DynamicPageHeader
                class="noPadding"
                pinnable="false"
            >
                <Panel
                    class="sapUiNoContentPadding"
                    backgroundDesign="Transparent"
                >
                    <c:Fragment
                        fragmentName="gestionesop.view.fragment.rag.HeaderSop"
                        type="XML"
                    />
                    <c:Fragment
                        fragmentName="gestionesop.view.fragment.rag.IconTabBar"
                        type="XML"
                    />
                </Panel>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <Wizard
                id="wizDetail"
                showNextButton="false"
            >
                <WizardStep title="{i18n>titleWizard1Rag}">
                    <c:Fragment
                        fragmentName="gestionesop.view.fragment.rag.wizard1.DatiIdentificativi"
                        type="XML"
                    />
                </WizardStep>
                <WizardStep title="{i18n>titleWizard2Rag}" />
                <WizardStep title="{i18n>titleWizard3Rag}" />
                <WizardStep title="{i18n>titleWizard4Rag}" />
                <WizardStep title="{i18n>titleWizard5Rag}" />
                <WizardStep title="{i18n>titleWizard6Rag}" />
                <WizardStep title="{i18n>titleWizard7Rag}" />
            </Wizard>
        </f:content>
        <f:footer>
            <OverflowToolbar>
                <c:Fragment
                    fragmentName="gestionesop.view.fragment.rag.FooterToolbar"
                    type="XML"
                />
            </OverflowToolbar>
        </f:footer>
    </f:DynamicPage>
</mvc:View>
