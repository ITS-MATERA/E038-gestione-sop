<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:l="sap.ui.layout"
>
    <Panel
        backgroundDesign="Transparent"
        visible="{= ${Sop>/Zwels} === 'ID6' || ${Sop>/Zwels} === 'ID10' ? true : false}"
    >
        <headerToolbar>
            <OverflowToolbar>
                <Title text="{i18n>titleIntermediario1}" />
            </OverflowToolbar>
        </headerToolbar>
        <content>
            <l:Grid
                defaultSpan="XL12 L12 M12 S12"
                hSpacing="0.5"
            >
                <Text
                    text="{i18n>labelIban}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zibani}"
                    maxLength="34"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelBic}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zbici}"
                    maxLength="11"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelCoordEstere}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zcoordesti}"
                    maxLength="35"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Title text="{i18n>titleDatiAltraBanca}:" />
                <Text
                    text="{i18n>labelDenBanca}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData
                            span="XL2 L3 M5 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zdenbancai}"
                    maxLength="70"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelClearingSystemid}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zclearsysti}"
                    maxLength="5"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Title text="{i18n>titleIndirizzoAltraBanca}:" />
                <Text
                    text="{i18n>labelVia}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData
                            span="XL1 L3 M5 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zstrasi}"
                    maxLength="35"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelCivico}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL1 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zcivicoi}"
                    maxLength="5"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelCitta}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData
                            span="XL1 L3 M5 S12"
                            linebreak="true"
                        />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zort01i}"
                    maxLength="35"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelProvincia}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL1 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zregioi}"
                    maxLength="3"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelCap}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL1 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zpstlzi}"
                    maxLength="10"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
                <Text
                    text="{i18n>labelNazione}:"
                    class="sapUiTinyMarginTop sapUiLargeMarginEnd"
                >
                    <layoutData>
                        <l:GridData span="XL1 L3 M5 S12" />
                    </layoutData>
                </Text>
                <Input
                    editable="false"
                    value="{Sop>/Zland1i}"
                    maxLength="3"
                >
                    <layoutData>
                        <l:GridData span="XL2 L3 M5 S12" />
                    </layoutData>
                </Input>
            </l:Grid>
        </content>
    </Panel>
</c:FragmentDefinition>
