<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <SelectDialog
        id="sdRagioneria"
        noDataText="{i18n>noData}"
        title="{i18n>labelRagioneriaDestinataria}"
        search="onValueHelpChange"
        confirm="onValueHelpRagioneriaClose"
        cancel="onValueHelpRagioneriaClose"
        app:FilterValueHelp="Description"
        items="{
          path: 'Ragioneria>/'
        }"
    >
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{Ragioneria>Zragdest}"
        />
    </SelectDialog>
</c:FragmentDefinition>
