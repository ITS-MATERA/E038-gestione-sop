<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <SelectDialog
        id="sdCos"
        noDataText="{i18n>noData}"
        title="{i18n>labelCos}"
        search="onValueHelpChange"
        confirm="onValueHelpCosClose"
        cancel="onValueHelpCosClose"
        app:FilterValueHelp="Zcos"
        items="{
          path: 'Cos>/'
        }"
    >
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{Cos>Zcos}"
            description="{Cos>ZcosDesc}"
        />
    </SelectDialog>
</c:FragmentDefinition>
