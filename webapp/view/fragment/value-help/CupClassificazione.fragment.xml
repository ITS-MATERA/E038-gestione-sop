<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <SelectDialog
        id="sdCup"
        noDataText="{i18n>noData}"
        title="{i18n>labelCup}"
        search="onValueHelpChange"
        confirm="onValueHelpCupClassificazioneClose"
        cancel="onValueHelpCupClassificazioneClose"
        app:FilterValueHelp="Cup"
        items="{
          path: 'Cup>/'
		    }"
    >
        <StandardListItem
            iconDensityAware="false"
            iconInset="false"
            title="{Cup>Cup}"
        />
    </SelectDialog>
</c:FragmentDefinition>
