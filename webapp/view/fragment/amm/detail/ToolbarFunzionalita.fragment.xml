<c:FragmentDefinition
    xmlns="sap.m"
    xmlns:c="sap.ui.core"
>
    <Button
        text="{i18n>btnAnnullamentoSop}"
        type="Emphasized"
        enabled="{= ${Sop>/ZcodStatosop} === '00' ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false ? true : false}"
        press="onAnnulla"
    />
    <Button
        text="{i18n>btnInvioFirma}"
        type="Emphasized"
        enabled="{= ${Sop>/ZcodStatosop} === '00' ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false || ${Utility>/Function} === 'InvioFirma' ? true : false}"
        press="onInviaFirma"
    />
    <Button
        text="{i18n>btnRevocaInvioFirma}"
        type="Emphasized"
        enabled="{= ${Utility>/EnableBtnRevocaInvioFirma} ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false || ${Utility>/EnableRevocaInvioFirma} ? true : false}"
        press="onRevocaInvioFirma"
    />
    <Button
        text="{i18n>btnFirmaSop}"
        type="Emphasized"
        enabled="{= ${Utility>/EnableBtnFirma} ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false || ${Utility>/EnableFirma} ? true : false}"
        press="onFirma"
    />
    <Button
        text="{i18n>btnRichiamo}"
        type="Emphasized"
        enabled="{= ${Utility>/EnableBtnRichiamo} ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false || ${Utility>/EnableBtnRichiamo} ? true : false}"
        press="onRichiamo"
    />
    <Button
        text="{i18n>btnRegistraRichiestaAnnullamento}"
        type="Emphasized"
        enabled="{= ${Utility>/EnableBtnRegistrazioneRichAnn} ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false || ${Utility>/EnableRegistrazioneRichAnn} ? true : false}"
        press="onRegistraRichAnn"
    />
    <Button
        text="{i18n>btnCancellazioneRichiestaAnnullamento}"
        type="Emphasized"
        enabled="{= ${Utility>/EnableBtnCancellazioneRichAnn} ? true : false}"
        visible="{= ${Utility>/RemoveFunctionButtons} === false || ${Utility>/EnableCancellazioneRichAnn} ? true : false}"
        press="onCancellaRichAnn"
    />
</c:FragmentDefinition>
